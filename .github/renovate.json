{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "baseBranches": [
    "master"
  ],
  "lockFileMaintenance": {
    "enabled": false
  },
  "packageRules": [
    {
      "matchPackageNames": ["boto3"],
      "matchUpdateTypes": ["minor", "major"],
      "description": "Only update boto3 when minor or major versions change"
    }
  ]
}
